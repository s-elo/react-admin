.menu {
  width: 100%;
  height: fit-content;
}
.menu .logo {
  width: 100%;
  height: 80px;
  padding: 10px;
  margin-top: 20px;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.menu .logo img {
  width: 50px;
  height: 50px;
  transition: 1s;
}
.menu .logo img:hover {
  transform: rotate(360deg);
}
.menu .logo .banner {
  font-size: 18px;
  font-weight: bold;
}
